{
    "name": "qbus/autoflush",
    "type": "typo3-cms-extension",
    "description": "Automatic cache flush for menus and levelmedia",
    "keywords": [
        "TYPO3",
        "extension"
    ],
    "homepage": "https://www.qbus.de",
    "authors": [
        {
            "name": "Benjamin Franzke",
            "email": "bfr@qbus.de",
            "role": "Developer",
            "homepage": "https://www.qbus.de"
        }
    ],
    "license": "GPL-2.0+",
    "require": {
        "typo3/cms-core": "~6.2.14 || ^7.4.0 || >=8.5 <8.7"
    },
    "autoload": {
        "psr-4": {
            "Qbus\\Autoflush\\": "Classes"
        }
    },
    "replace": {
        "autoflush": "self.version",
        "typo3-ter/autoflush": "self.version"
    }
}
