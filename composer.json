{
    "name": "qbus/autoflush",
    "type": "typo3-cms-extension",
    "description": "Automatic cache flush for menus and levelmedia",
    "keywords": [
        "TYPO3",
        "extension"
    ],
    "homepage": "https://www.qbus.de",
    "authors": [
        {
            "name": "Benjamin Franzke",
            "email": "bfr@qbus.de",
            "role": "Developer",
            "homepage": "https://www.qbus.de"
        }
    ],
    "license": "GPL-2.0+",
    "require": {
        "typo3/cms-core": "^8.5.0 || ^9.5.0 || ^10.4.0",
        "typo3/cms-extbase": "^8.5.0 || ^9.5.0 || ^10.4.0",
        "typo3/cms-frontend": "^8.5.0 || ^9.5.0 || ^10.4.0",
        "typo3/cms-backend": "^8.5.0 || ^9.5.0 || ^10.4.0",
        "typo3/cms-extensionmanager": "^8.5.0 || ^9.5.0 || ^10.4.0"
    },
    "autoload": {
        "psr-4": {
            "Qbus\\Autoflush\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "autoflush"
        }
    },
    "replace": {
        "typo3-ter/autoflush": "self.version"
    }
}
